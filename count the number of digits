import java.util.Scanner;
class Main
{
	public static void main(String[] args)
	{
	    Scanner scn=new Scanner(System.in);
	    int a=scn.nextInt();
	    int arr[]=new int[a];
	    for(int i=0;i<arr.length;i++)
	    {
	        arr[i]=scn.nextInt();
	    }
	    int count=0;
	    for(int i=0;i<arr.length;i++)
	    {
	        if(arr[i]!=0)
	        {
	            count++;
	        }
	    }
	    System.out.println("number of digits "+ count);
	}
}
