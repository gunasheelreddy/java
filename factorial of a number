import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    System.out.print("Enter a number: ");
		Scanner scn = new Scanner(System.in);
		int a= scn.nextInt();
		int fact=1;
		for(int i=1;i<=a;i++)
		{
		    fact=fact*i;
		}
		System.out.println("The factorial of given number is: "+fact);
	}
}
